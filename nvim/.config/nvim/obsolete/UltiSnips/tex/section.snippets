# --------------------------- Section environments --------------------------- #

snippet part "Part" b
\part{${1:${VISUAL:Part}}}%
$0
endsnippet

snippet chap "Chapter" b
\chapter{${1:${VISUAL:Chapter}}}%
$0
endsnippet

snippet sec "Section" b
\section{${1:${VISUAL:Section}}}%
$0
endsnippet

snippet sec* "Section" b
